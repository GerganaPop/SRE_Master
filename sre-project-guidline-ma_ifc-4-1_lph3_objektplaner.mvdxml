                                </AttributeRule>
                                <AttributeRule AttributeName="Axis">
                                  <EntityRules>
                                    <EntityRule EntityName="IfcDirection"/>
                                  </EntityRules>
                                </AttributeRule>
                                <AttributeRule AttributeName="RefDirection">
